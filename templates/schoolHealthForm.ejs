        <div class="container-fluid">
            <div class="form-card-title">
                <strong>SCHOOL HEALTH EXAMINATION CARD</strong>
            </div>
        </div>
        <div class=" container-fluid header-content">
            <form class="form school-health-form" action="">
                <div class="container-fluid school-form">
                    <div class="container-fluid left">
                        <div class="row nameList">
                            <div class="nameField">
                                <label class="nameTitle" for="">Name:</label>
                                <div class='nameDiv'>
                                    <input class="name-field" value="<%=student.lastName%>">
                                    <label for="lname">Last</label>
                                </div>
                                <div class='nameDiv'>
                                    <input class="name-field" value="<%=student.firstName%>">
                                    <label for="lname">First</label>
                                </div>
                                <div class='nameDiv'>
                                    <input class="name-field" value="<%=student.middleName%>">
                                    <label for="lname">Middle</label>
                                </div>
                            </div>
                        </div>
                        <div class="row lrnList">
                            <div class="lrnField">
                                <label class="lrnTitle" for="lrn">LRN</label>
                                <div class='lrnDiv'>
                                    <input class="lrn-field" value="<%=student.lrn%>">
                                </div>
                            </div>
                        </div>
                        <div class="row dobList">
                            <div class="dobField">
                                <label class="dobTitle" for="">Date of Birth:</label>
                                <div class='dobDiv'>
                                    <input class="dob-field" value="<%=student.dob.month%>">
                                    <label for="lname">Month</label>
                                </div>
                                <div class='dobDiv'>
                                    <input class="dob-field" value="<%=student.dob.day%>">
                                    <label for="lname">Day</label>
                                </div>
                                <div class='dobDiv'>
                                    <input class="dob-field" value="<%=student.dob.year%>">
                                    <label for="lname">Year</label>
                                </div>
                            </div>
                        </div>
                        <div class="row bpList">
                            <div class="bpField">
                                <label class="bpTitle" for="">Birthplace:</label>
                                <div class='bpDiv'>
                                    <input class="bp-field" value="<%=student.birthPlace%>">
                                </div>
                            </div>
                        </div>
                        <div class="row parentList">
                            <div class="parentField">
                                <label class="parentTitle" for="">Parent/Guardian:</label>
                                <div class='parentDiv'>
                                    <input class="parent-field" value="<%=student.contactPerson%>">
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="container-fluid right">
                        <div class="row idList">
                            <div class="idField">
                                <label class="idTitle" for="">School ID:</label>
                                <div class='idDiv'>
                                    <input class="id-field" value="<%=student.schoolId%>">
                                </div>
                            </div>
                        </div>
                        <div class="row regionList">
                            <div class="regionField">
                                <label class="regionTitle" for="">Region:</label>
                                <div class='regionDiv'>
                                    <input class="region-field" value="IX" disabled>
                                </div>
                            </div>
                        </div>
                        <div class="row divisionList">
                            <div class="divisionField">
                                <label class="divisionTitle" for="">Division:</label>
                                <div class='divisionDiv'>
                                    <input class="division-field" value="ZAMBOANGA CITY" disabled>
                                </div>
                            </div>
                        </div>
                        <div class="row phoneList">
                            <div class="phoneField">
                                <label class="phoneTitle" for="">Telephone No.:</label>
                                <div class='phoneDiv'>
                                    <input class="phone-field">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="container-fluid">
                    <div class="row addressList">
                        <div class="addressField">
                            <label class="addressTitle" for="">Address</label>
                            <div class='addressDiv'>
                                <input class="address-field" value="<%=student.address%>">
                            </div>
                        </div>
                    </div>
                </div>
            </form>
        </div>
